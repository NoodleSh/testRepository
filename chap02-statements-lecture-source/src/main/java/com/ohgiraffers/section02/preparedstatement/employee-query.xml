<?xml version="1.0" encoding="UTF-8" standalone = "no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment/>
    <entry key="selectEmpByeFamilyName">
        <!-- 성씨로 이름 조회-->
        SELECT
            E.*
        FROM
            EMPLOYEE E
        WHERE
            E.EMP_NAME LIKE CONCAT(?, '%')
    </entry>
</properties>
